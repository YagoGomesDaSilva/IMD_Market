<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserAuthentication.RegisterActivity">

    <EditText
        android:id="@+id/et_PassWordName_SignOn"
        android:layout_width="234dp"
        android:layout_height="63dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="@string/et_PassWordName"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_UserName_SignOn" />

    <Button
        android:id="@+id/btn_Back_SignOn"
        android:layout_width="127dp"
        android:layout_height="55dp"
        android:text="@string/btn_Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Register_SignOn"
        app:layout_constraintVertical_bias="0.257" />

    <Button
        android:id="@+id/btn_Register_SignOn"
        android:layout_width="125dp"
        android:layout_height="59dp"
        android:layout_marginTop="116dp"
        android:text="@string/btn_SignOn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_PassWordName_SignOn" />

    <EditText
        android:id="@+id/et_UserName_SignOn"
        android:layout_width="233dp"
        android:layout_height="67dp"
        android:layout_marginTop="220dp"
        android:ems="10"
        android:hint="@string/et_UserName"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>